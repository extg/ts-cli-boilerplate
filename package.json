{
  "name": "project_name",
  "description": "Fast start for ts project!",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "project_name": "./bin/index.js"
  },
  "scripts": {
    "start": "concurrently \"npm:start:types\" \"npm:start:js\" \"npm:type-check:watch\"",
    "start:types": "tsc --emitDeclarationOnly",
    "start:js": "babel -w ./src -d ./lib --extensions \".ts,.js\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prebuild": "rm -rf ./lib",
    "build": "concurrently \"npm:build:types\" \"npm:build:js\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel ./src -d ./lib --extensions \".ts,.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "preversion": "npm run build",
    "postversion": "npm run release",
    "release": "git push --follow-tags # && npm run build && npm publish lib"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/axios": "^0.14.0",
    "@types/cosmiconfig": "^5.0.3",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.13",
    "@types/micromatch": "^3.1.0",
    "@types/node": "^10.12.18",
    "@types/package-json": "^5.0.0",
    "@types/ramda": "^0.25.47",
    "@types/shelljs": "^0.8.3",
    "@types/warning": "^3.0.0",
    "@types/yargs": "^12.0.8",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "cosmiconfig": "^5.0.7",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "ramda": "^0.26.1",
    "shelljs": "^0.8.3",
    "warning": "^4.0.3",
    "yargs": "^12.0.5"
  }
}
